<UserControl x:Class="Controls.HistoryResultIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Width="20" Height="22" x:Name="HistoryResultIconX">
    
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Images/历史记录结果图标.png" Stretch="None"/>
        </Grid.Background>
        <TextBlock Text="{Binding Number, ElementName=HistoryResultIconX}" 
                   Foreground="White" FontFamily="/Controls;component/Fonts/ARIALUNI.TTF#Arial Unicode MS"
                   HorizontalAlignment="Center" Margin="0,2,0,0" FontSize="12"/>
    </Grid>
</UserControl>
