<UserControl x:Class="Controls.MultiSelectButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Height="29" Width="28">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="StyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="GridOfMultiSelectButton" Style="{StaticResource MultiSelectButton_NormalEffect}"
          MouseEnter="MouseEnterAction"
          MouseLeave="MouseLeaveAction"
          MouseLeftButtonDown="MouseLeftButtonDownAction"
          MouseLeftButtonUp="MouseLeftButtonUpAction">
        
        <Image x:Name="MultiSelectImage" Source="Images/全.png" Stretch="None"/>        
    </Grid>
</UserControl>
